{
  "name": "mufasa",
  "version": "0.0.200",
  "main": "./dist/mufasa.js",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/monode-dev/Mufasa.git"
  },
  "scripts": {
    "build": "tsc",
    "pub": "sucrase-node pub.js"
  },
  "files": [
    "dist/",
    "dist/Plugins/"
  ],
  "exports": {
    ".": {
      "types": "./dist/Mufasa.d.ts",
      "default": "./dist/Mufasa.js"
    },
    "./solid": {
      "types": "./dist/Plugins/Solid.d.ts",
      "default": "./dist/Plugins/Solid.js"
    },
    "./capacitor": {
      "types": "./dist/Plugins/Capacitor.d.ts",
      "default": "./dist/Plugins/Capacitor.js"
    },
    "./firebase": {
      "types": "./dist/Plugins/Firebase.d.ts",
      "default": "./dist/Plugins/Firebase.js"
    },
    "./compress-image": {
      "types": "./dist/Plugins/CompressImage.d.ts",
      "default": "./dist/Plugins/CompressImage.js"
    }
  },
  "dependencies": {
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "@capacitor/filesystem": "^5.0.0",
    "firebase": "^10.0.0",
    "solid-js": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@types/uuid": "^9.0.8",
    "semver": "^7.5.4",
    "sucrase": "^3.32.0"
  }
}